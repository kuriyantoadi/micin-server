
# INI ADALA BAGIAN ALIAS   #


alias lampp=' /opt/lampp/lampp'
alias conip=' nano /etc/network/interfaces'
alias resip=' /etc/init.d/networking restart'
alias htdocs=' cd /opt/lampp/htdocs'
alias config=' nano /opt/lampp/htdocs/library/config.php'
alias resxa=' /opt/lampp/lampp restart'
alias staxa=' /opt/lampp/lampp status'
alias rebind=' service networking restart'
alias syslog='tail -f /var/log/syslog'
