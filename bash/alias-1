
# INI ADALA BAGIAN ALIAS   #


alias lampp='sudo /opt/lampp/lampp'
alias conip='sudo nano /etc/network/interfaces'
alias resip='sudo /etc/init.d/networking restart'
alias htdocs='sudo cd /opt/lampp/htdocs'
alias config='suddo nano /opt/lampp/htdocs/library/config.php'
alias resxa='sudo /opt/lampp/lampp restart'
alias staxa='sudo /opt/lampp/lampp status'
alias rebind='sudo service networking restart'
alias syslog='sudo /var/log/syslog'
